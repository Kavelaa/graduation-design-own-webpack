    <Byte ByteName="跳频方式" Type="Option">
      <Option Val="固定"/>
      <Option Val="帧间跳"/>
    </Byte>
    <Byte ByteName="方位扫描中心" Type="Range">
      <Range DataType="INT16" Precision="0.1" limit_lower="-180" limit_upper="180" Unit="度"/>
    </Byte>
  </Ins>
  <Ins InsName="雷达辐射">
    <Byte ByteName="扫描方式" Type="Option">
      <Option Val="广度扫描"/>
      <Option Val="深度扫描"/>
    </Byte>
    <Byte ByteName="辐射范围" Type="Range">
      <Range DataType="INT16" Precision="0.1" limit_lower="-180" limit_upper="180" Unit="度"/>
    </Byte>
  </Ins>
</DBItem>